    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install && cd .."
  },
  "keywords": [
    "client",
    "records",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "dotenv": "^16.6.1",
    "googleapis": "npm:null@*"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
